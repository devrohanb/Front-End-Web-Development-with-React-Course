  } else {\n      return <div></div>;\n    }\n  }\n\n  render() {\n    const dishSelected = this.props.dish;\n    return (\n      <div className=\"row\">\n        {this.renderDish(dishSelected)}\n        {this.renderComments(dishSelected)}\n      </div>\n    );\n  }\n}\n\nexport default DishDetails;\n",{"ruleId":"32","severity":1,"message":"33","line":6,"column":8,"nodeType":"34","messageId":"35","endLine":6,"endColumn":19},{"ruleId":"32","severity":1,"message":"36","line":6,"column":3,"nodeType":"34","messageId":"35","endLine":6,"endColumn":11},{"ruleId":"32","severity":1,"message":"37","line":7,"column":3,"nodeType":"34","messageId":"35","endLine":7,"endColumn":11},{"ruleId":"38","severity":1,"message":"39","line":5,"column":3,"nodeType":"40","messageId":"41","endLine":7,"endColumn":4},"no-unused-vars","'DishDetails' is defined but never used.","Identifier","unusedVar","'CardText' is defined but never used.","'CardBody' is defined but never used.","no-useless-constructor","Useless constructor.","MethodDefinition","noUselessConstructor"]